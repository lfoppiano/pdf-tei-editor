{
  "login-dialog": "<sl-dialog label=\"Login\" class=\"login-dialog\" name=\"loginDialog\">\r\n  <div>\r\n    <sl-input name=\"username\" label=\"Username\" required></sl-input>\r\n    <br>\r\n    <sl-input type=\"password\" name=\"password\" label=\"Password\" required></sl-input>\r\n    <div name=\"message\" class=\"error-message\" style=\"color: red; margin-top: 10px;\"></div>\r\n  </div>\r\n  <sl-button type=\"submit\" name=\"submit\" slot=\"footer\" variant=\"primary\">Login</sl-button>\r\n</sl-dialog>",
  "logout-button": "<sl-tooltip content=\"Log out\">\r\n  <sl-button name=\"logoutButton\" size=\"small\">\r\n    <sl-icon name=\"power\"></sl-icon>\r\n  </sl-button>\r\n</sl-tooltip>",
  "extraction-buttons": "<sl-button-group name=\"extractionActions\" label=\"Extraction\" >\r\n  <sl-tooltip content=\"Upload a new PDF and extract references\">\r\n    <sl-button name=\"extractNew\" size=\"small\">\r\n      <sl-icon name=\"filetype-pdf\"></sl-icon>\r\n    </sl-button>\r\n  </sl-tooltip>\r\n  <sl-tooltip content=\"Extract from the current PDF into a new TEI version\">\r\n    <sl-button name=\"extractCurrent\" size=\"small\">\r\n      <sl-icon name=\"clipboard2-plus\"></sl-icon>\r\n    </sl-button>\r\n  </sl-tooltip>\r\n</sl-button-group>",
  "extraction-dialog": "<sl-dialog name=\"extractionOptions\" label=\"Extraction options\">\r\n  <div class=\"dialog-column\">\r\n    <sl-input name=\"doi\" label=\"DOI\" size=\"small\" help-text=\"Please enter the DOI of the document to add document metadata\"></sl-input>\r\n    <div>\r\n      <sl-select name=\"collectionName\" label=\"Collection\" size=\"small\" help-text=\"Choose the collection to which the extraction will be added\"></sl-select>\r\n      <!--<sl-button name=\"newCollectionBtn\" label=\"New collection\" size=\"small\">New collection</sl-button>-->\r\n    </div>\r\n    <sl-select name=\"modelIndex\" label=\"Model\" size=\"small\" help-text=\"Choose the model configuration used for the extraction\" value=\"llamore\">\r\n      <sl-option value=\"llamore\">Llamore</sl-option>\r\n    </sl-select>\r\n    <div name=\"dynamicOptions\" class=\"dialog-column\">\r\n      <!-- Dynamic extractor-specific options will be inserted here -->\r\n    </div>\r\n  </div>\r\n  <sl-button slot=\"footer\" name=\"cancel\" variant=\"neutral\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"submit\" variant=\"primary\">Extract</sl-button>  \r\n</sl-dialog>",
  "file-selection-drawer": "<!-- File selection drawer -->\r\n<sl-drawer name=\"fileDrawer\" label=\"Select Document\" placement=\"start\" style=\"--size: 400px;\">\r\n  <div style=\"display: flex; flex-direction: column; height: 100%;\">\r\n    \r\n    <!-- Filter area at top -->\r\n    <div style=\"padding: 1rem; border-bottom: 1px solid var(--sl-color-neutral-200); flex-shrink: 0;\">\r\n      \r\n      <!-- Variant selection -->\r\n      <div style=\"margin-bottom: 1rem;\">\r\n        <label style=\"display: block; margin-bottom: 0.5rem; font-weight: bold;\">Variant:</label>\r\n        <sl-select name=\"variantSelect\" placeholder=\"Select variant...\" size=\"small\" hoist>\r\n          <!-- Will be populated dynamically -->\r\n        </sl-select>\r\n      </div>\r\n      \r\n      <!-- Label filter input -->\r\n      <div>\r\n        <label style=\"display: block; margin-bottom: 0.5rem; font-weight: bold;\">Filter by label:</label>\r\n        <sl-input name=\"labelFilter\" placeholder=\"Search documents...\" size=\"small\" clearable>\r\n          <sl-icon name=\"search\" slot=\"prefix\"></sl-icon>\r\n        </sl-input>\r\n      </div>\r\n      \r\n    </div>\r\n    \r\n    <!-- Tree area - fills remaining space -->\r\n    <div style=\"flex: 1; padding: 1rem; overflow-y: auto; min-height: 0; display: block; width: 100%;\">\r\n      <sl-tree name=\"fileTree\" selection=\"leaf\" style=\"width: 100%; min-height: 200px; display: block;\">\r\n        <!-- Will be populated dynamically -->\r\n      </sl-tree>\r\n    </div>\r\n    \r\n  </div>\r\n  \r\n  <!-- Footer with close button -->\r\n  <div slot=\"footer\" style=\"display: flex; justify-content: flex-end; padding: 1rem; border-top: 1px solid var(--sl-color-neutral-200);\">\r\n    <sl-button name=\"closeDrawer\" variant=\"default\" size=\"small\">\r\n      Close\r\n    </sl-button>\r\n  </div>\r\n</sl-drawer>",
  "file-drawer-button": "<!-- Hamburger button for toolbar -->\r\n <sl-tooltip content=\"Select documents for viewing and editing\">\r\n  <sl-button name=\"fileDrawerTrigger\" size=\"small\">\r\n    <sl-icon name=\"list\"></sl-icon>\r\n  </sl-button>\r\n </sl-tooltip>",
  "file-selection": "<sl-select name=\"variant\" size=\"small\" label=\"Model Variant\" hoist></sl-select>  \r\n<sl-select name=\"pdf\" size=\"small\" label=\"PDF\" hoist></sl-select>\r\n<sl-select name=\"xml\" size=\"small\" label=\"XML file version\" hoist></sl-select>\r\n<sl-select name=\"diff\" size=\"small\" label=\"Compare with version\" hoist></sl-select>",
  "info-dialog": "<sl-drawer name=\"infoDrawer\" label=\"User Manual\" style=\"--size: 80ch;\">\r\n  <div name=\"content\" class=\"markdown-body\">\r\n    <!-- content will be dynamically updated-->\r\n  </div>\r\n  <div slot=\"footer\" style=\"display: flex; width: 100%; justify-content: space-between; align-items: center;\">\r\n    <sl-button name=\"editGitHubBtn\" variant=\"default\">\r\n      <sl-icon name=\"github\" slot=\"prefix\"></sl-icon>Edit on GitHub</sl-button>\r\n    <div style=\"display: flex; gap: 8px;\">\r\n      <sl-button name=\"backBtn\" variant=\"default\" disabled>\r\n        <sl-icon name=\"arrow-left\" slot=\"prefix\"></sl-icon>Back</sl-button>\r\n      <sl-button name=\"homeBtn\" variant=\"default\">\r\n        <sl-icon name=\"house\" slot=\"prefix\"></sl-icon>Home</sl-button>\r\n      <sl-button name=\"forwardBtn\" variant=\"default\" disabled>\r\n        <sl-icon name=\"arrow-right\" slot=\"prefix\"></sl-icon>Forward</sl-button>\r\n      <sl-button name=\"closeBtn\" variant=\"primary\">Close</sl-button>\r\n    </div>\r\n  </div>\r\n</sl-drawer>",
  "about-button": "<sl-button name=\"aboutBtn\" slot=\"footer\" variant=\"default\">\r\n  <sl-icon name=\"info-circle\" slot=\"prefix\"></sl-icon>About\r\n</sl-button>",
  "info-toolbar-button": "<sl-tooltip content=\"User Manual\">\r\n  <sl-button name=\"editInstructions\" size=\"small\">\r\n    <sl-icon name=\"info-circle\"></sl-icon>\r\n  </sl-button>\r\n</sl-tooltip>",
  "move-files-dialog": "<sl-dialog name=\"moveFilesDialog\" label=\"Move Files to Collection\">\r\n  <p>\r\n    Select a collection to move the current PDF and XML files to.\r\n  </p>\r\n  <div style=\"display: flex; align-items: flex-end; gap: 0.5rem;\">\r\n    <sl-select name=\"collectionName\" label=\"Collection\" required style=\"flex-grow: 1;\" hoist></sl-select>\r\n    <sl-button name=\"newCollectionBtn\" variant=\"default\" outline>\r\n      <sl-icon name=\"plus-lg\" slot=\"prefix\"></sl-icon> New\r\n    </sl-button>\r\n  </div>\r\n  <sl-button slot=\"footer\" name=\"cancel\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"submit\" variant=\"primary\">Move</sl-button>\r\n</sl-dialog>",
  "prompt-editor": "<sl-dialog name=\"promptEditor\" label=\"Edit prompt\" class=\"dialog-big\">\r\n  <p>Here you can edit the LLM prompt specific to the reference extraction.</p>\r\n  <div class=\"dialog-column\">\r\n    <div class=\"dialog-row\">\r\n      <sl-input name=\"label\" help-text=\"A short description of the prompt additions\"></sl-input>\r\n      <sl-dropdown>\r\n        <sl-button slot=\"trigger\" caret></sl-button>\r\n        <sl-menu name=\"labelMenu\"></sl-menu>\r\n      </sl-dropdown>\r\n    </div>\r\n    <sl-select name=\"extractorSelect\" label=\"Extractors\" multiple clearable placeholder=\"Select extractors...\" help-text=\"Choose which extractors can use this prompt\">\r\n    </sl-select>\r\n    <sl-textarea name=\"text\" rows=\"20\"></sl-textarea>\r\n  </div>\r\n  <sl-button slot=\"footer\" name=\"cancel\" variant=\"neutral\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"delete\" disabled variant=\"danger\">Delete prompt</sl-button>\r\n  <sl-button slot=\"footer\" name=\"duplicate\" variant=\"secondary\">Duplicate prompt</sl-button>\r\n  <sl-button slot=\"footer\" name=\"submit\" variant=\"primary\">Save &amp; Close</sl-button>\r\n</sl-dialog>",
  "prompt-editor-button": "<sl-tooltip content=\"Edit the prompt instructions\">\r\n  <sl-button name=\"editInstructions\" size=\"small\">\r\n    <sl-icon name=\"pencil-square\"></sl-icon>\r\n  </sl-button>\r\n</sl-tooltip>",
  "document-action-buttons": "<span class=\"hbox-with-gap toolbar-content\">\r\n  <!-- Document button group -->\r\n  <sl-button-group label=\"Document\" name=\"documentActions\">\r\n\r\n    <!-- create new version -->\r\n    <sl-tooltip content=\"Duplicate current document to make changes to a new version\">\r\n      <sl-button name=\"createNewVersion\" size=\"small\" disabled>\r\n        <sl-icon name=\"copy\"></sl-icon>\r\n      </sl-button>\r\n    </sl-tooltip>  \r\n\r\n    <!-- upload, not implemented yet -->\r\n    <sl-tooltip content=\"Upload document\">\r\n      <sl-button name=\"upload\" size=\"small\">\r\n        <sl-icon name=\"cloud-upload\"></sl-icon>\r\n      </sl-button>\r\n    </sl-tooltip>    \r\n\r\n    <!-- download -->\r\n    <sl-tooltip content=\"Download XML document\">\r\n      <sl-button name=\"download\" size=\"small\" disabled>\r\n        <sl-icon name=\"cloud-download\"></sl-icon>\r\n      </sl-button>\r\n    </sl-tooltip>   \r\n\r\n    <!-- delete -->\r\n    <sl-button-group>\r\n      <sl-dropdown placement=\"bottom-end\">\r\n        <sl-button name=\"deleteBtn\" size=\"small\" slot=\"trigger\" caret>\r\n          <sl-icon name=\"trash3\"></sl-icon>\r\n        </sl-button>\r\n        <sl-menu>\r\n          <sl-menu-item name=\"deleteCurrentVersion\">Delete current version</sl-menu-item>\r\n          <sl-menu-item name=\"deleteAllVersions\">Delete all versions</sl-menu-item>\r\n          <sl-menu-item name=\"deleteAll\">Delete PDF and XML</sl-menu-item>\r\n        </sl-menu>\r\n      </sl-dropdown>\r\n    </sl-button-group>\r\n\r\n    <!-- document revision -->\r\n    <sl-tooltip content=\"Save document revision\">\r\n      <sl-button name=\"saveRevision\" size=\"small\" disabled>\r\n        <sl-icon name=\"save\"></sl-icon>\r\n      </sl-button>\r\n    </sl-tooltip>    \r\n    \r\n  </sl-button-group>\r\n\r\n  <!-- TEI -->\r\n  <sl-button-group label=\"TEI\" name=\"teiActions\">\r\n\r\n    <!-- validate -->\r\n    <sl-tooltip content=\"Validate the document\">\r\n      <sl-button name=\"validate\" size=\"small\" disabled>\r\n        <sl-icon name=\"file-earmark-check\"></sl-icon>\r\n      </sl-button> \r\n    </sl-tooltip>\r\n\r\n  </sl-button-group>\r\n</span>",
  "new-version-dialog": "<sl-dialog name=\"newVersionDialog\" label=\"Create new version\">\r\n  <div class=\"dialog-column\">\r\n    <sl-input name=\"versionName\" label=\"Version Name\" size=\"small\" help-text=\"Provide a short name for the version (required)\"></sl-input>\r\n    <sl-input name=\"persId\" label=\"Initials\" size=\"small\" help-text=\"Your initials (required)\"></sl-input>\r\n    <sl-input name=\"persName\" label=\"Editor Name\" size=\"small\" help-text=\"Your name, if this is your first edit on this document\"></sl-input>\r\n    <sl-input name=\"editionNote\" label=\"Description\" size=\"small\" help-text=\"Description of this version (optional)\"></sl-input>\r\n  </div>\r\n  <sl-button slot=\"footer\" name=\"cancel\" variant=\"neutral\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"submit\" variant=\"primary\">Create new version</sl-button>  \r\n</sl-dialog>",
  "save-revision-dialog": "<sl-dialog name=\"newRevisionChangeDialog\" label=\"Add a change description\">\r\n  <div class=\"dialog-column\">\r\n    Document changes to this document in the TEI header\r\n    <sl-input name=\"persId\" label=\"Initials\" size=\"small\" help-text=\"Your initials (required)\"></sl-input>\r\n    <sl-input name=\"persName\" label=\"Editor Name\" size=\"small\" help-text=\"Your name, if this is your first edit on this document\"></sl-input>\r\n    <sl-input name=\"changeDesc\" label=\"Description\" size=\"small\" help-text=\"Description of the change\"></sl-input>\r\n  </div>\r\n  <sl-button slot=\"footer\" name=\"cancel\" variant=\"neutral\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"submit\" variant=\"primary\">Add</sl-button>  \r\n</sl-dialog>",
  "tei-wizard-button": "<sl-tooltip content=\"Enhance TEI, i.e. add missing attributes\">\r\n<sl-button name=\"teiWizard\" size=\"small\">\r\n    <sl-icon name=\"magic\"></sl-icon>\r\n</sl-button>\r\n</sl-tooltip>",
  "tei-wizard-dialog": "<sl-dialog name=\"teiWizardDialog\" label=\"TEI Wizard\" class=\"dialog-width\" style=\"--width: 50vw;\">\r\n  <div name=\"enhancementList\"></div>\r\n  <sl-button slot=\"footer\" name=\"selectAll\">Select All</sl-button>\r\n  <sl-button slot=\"footer\" name=\"selectNone\">Select None</sl-button>\r\n  <sl-button slot=\"footer\" name=\"cancel\" variant=\"neutral\">Cancel</sl-button>\r\n  <sl-button slot=\"footer\" name=\"executeBtn\" variant=\"primary\">Execute</sl-button>\r\n</sl-dialog>"
}